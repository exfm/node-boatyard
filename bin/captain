#!/usr/bin/env node

"use strict";

var nconf = require('nconf'),
    suck = require('../'),
    Captain = suck.Captain;

nconf.argv()
    .env()
    .use('memory');

nconf.defaults({
    'size': 100000,
    'partitioner': '../examples/partitioner.js'
});

var captain = new Captain(Number(nconf.get('size')));
captain.startShip(require(nconf.get('partitioner')));